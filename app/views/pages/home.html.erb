<% unless user_signed_in? %>

	<% unless @tasker_view %>

		<%= render 'pages/basic_landing_page' %>

	<%else%>

		<%= render 'pages/tasker_landing_page' %>

	<%end%>

<%else%>
	
	<% if current_user.is_tasker %>

		<%= render 'users/tasker_dashboard' %>

	<%else%>

		<%= render 'users/non_tasker_dashboard' %>

	<%end%>
	
<%end%>

